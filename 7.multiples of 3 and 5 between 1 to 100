for (i in 1:100) {
  if (i %% 3 == 0 && i %% 5 == 0) {
    cat("FizzBuzz", "\n")
  } else if (i %% 3 == 0) {  # Check for multiples of 3
    cat("Fizz", "\n")
  } else if (i %% 5 == 0) {  # Check for multiples of 5
    cat("Buzz", "\n")
  } else {
    cat(i, "\n")  # Print the number if not a multiple of 3 or 5
  }
}

