matrix_by_rows <- matrix(1:20, nrow = 5, ncol = 4, byrow = TRUE)
row.names(matrix_by_rows) <- c("Row1", "Row2", "Row3", "Row4", "Row5")
colnames(matrix_by_rows) <- c("Col1", "Col2", "Col3", "Col4")
matrix_with_labels <- matrix(letters[1:9], nrow = 3, ncol = 3, byrow = TRUE)
rownames(matrix_with_labels) <- c("RowA", "RowB", "RowC")
colnames(matrix_with_labels) <- c("ColX", "ColY", "ColZ")
matrix_by_columns <- matrix(21:24, nrow = 2, ncol = 2, byrow = FALSE)
rownames(matrix_by_columns) <- c("RowX", "RowY")
colnames(matrix_by_columns) <- c("ColAlpha", "ColBeta")
cat("5x4 Matrix filled by rows:\n")
print(matrix_by_rows)
cat("\n3x3 Matrix with labels:\n")
print(matrix_with_labels)
cat("\n2x2 Matrix filled by columns:\n")
print(matrix_by_columns)

