find_max_min <- function(vector) {
  max_value <- max(vector)
  min_value <- min(vector)
  
  return(c(max_value, min_value))
}
example_vector <- c(10, 5, 7, 20, 15, 3, 8, 12)
max_min_values <- find_max_min(example_vector)

cat("Maximum value:", max_min_values[1], "\n")
cat("Minimum value:", max_min_values[2], "\n")
